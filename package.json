{
  "name": "sikky",
  "version": "0.8.6",
  "description": "A very fast, and small sized TypeScript 2.0 Pre boilerplate. Tests in Mocha & Chai. TSLint for better code quality",
  "main": "dist/sikky.js",
  "jsnext:main": "dist/sikky.mjs",
  "author": "KFlash",
  "homepage": "https://github.com/kflash/sikky",
  "license": "MIT",
  "engines": {
    "node": ">= 5.4.1 < 6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kflash/sikky.git"
  },
  "bugs": {
    "url": "https://github.com/kflash/sikky/issues"
  },
  "keywords": [
    "es6",
    "es2015",
    "async",
    "await",
    "jsx",
    "tsx",
    "jsnext",
    "ecmascript",
    "npm",
    "react",
    "TypeScript",
    "microsoft",
    "tslint",
    "universal",
    "rollup",
    "tslint",
    "mocha",
    "chai",
    "boilerplate"
  ],
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "npm run lint:src && npm run build:dev && npm run build:prod",
    "build:dev": "cross-env NODE_ENV=development rollup -c rollup.config.js",
    "build:prod": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "clean": "rm -rf coverage/ dist/ build/ typings/",
    "clean:build": "rm -rf build/",
    "clean:dist": "rm -rf dist/",
    "lint:src": "tslint src/**/*.tsx src/**/*.ts --ignore-path .gitignore",
    "lint:tests": "tslint tests/**/*.tsx tests/**/*.ts --ignore-path .gitignore",
    "preinstall": "typings install",
    "prebuild": "tsc src/sikky.ts --sourcemap --outDir build -t es5 --m es6 --declaration --skipLibCheck",
    "pretest": "rollup -c rollup.mocha.config.js",
    "test": "npm run test:browser && npm run test:node",
    "test:browser": "cross-env NODE_ENV=test karma start karma.conf.js --no-auto-watch --single-run",
    "test:node": "cross-env NODE_ENV=test mocha ./build/test-bundle.js --compilers js:buble/register",
    "tdd": "cross-env NODE_ENV=test karma start karma.conf.js --auto-watch",
    "watch": "tsc src/sikky.ts --sourcemap --outDir build -t es5 --m es6 --declaration --skipLibCheck --w"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^3.4.22-alpha",
    "@types/sinon-chai": "^2.7.20-alpha",
    "chai": "^3.5.0",
    "cross-env": "^1.0.8",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-sinon-chai": "^1.2.0",
    "karma-coveralls": "^1.1.2",
    "karma-coverage": "^1.0.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-rollup-preprocessor": "^2.0.2",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.3",
    "lolex": "^1.5.0",
    "mocha": "^2.5.2",
    "rollup": "^0.31.2",
    "rollup-plugin-buble": "^0.11.0",
    "rollup-plugin-commonjs": "^3.0.0",
    "rollup-plugin-replace": "^1.1.0",
    "rollup-plugin-uglify": "^1.0.0",
    "rollup-plugin-multi-entry": "^1.4.0",
    "rollup-plugin-istanbul": "^1.0.0",
    "rollup-plugin-node-resolve": "^1.7.0",
    "rollup-plugin-typescript": "^0.7.5",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "source-map": "^0.5.6",
    "source-map-support": "^0.4.0",
    "tslint": "next",
    "typescript": "next",
    "typings": "^1.1.0"
  }
}
