{
  "name": "sikky",
  "version": "0.0.8",
  "description": "TypeScript 2.0 Pre boilerplate that aims to be the fastest and smallest. Uses Rollup for bundle, and Buble as the ES2015 compiler",
  "main": "dist/sikky.js",
  "scripts": {
    "clean:build": "rimraf build/",
    "test": "",
    "tsc": "./node_modules/.bin/tsc",
    "build": "npm run tsc && npm run build:dev && npm run build:prod",
    "build:dev": "rollup -c rollup.dev.config.ts",
    "build:prod": "rollup -c rollup.prod.config.ts",
    "lint": "tslint \"src/**/*.ts?(x)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kflash/sikky.git"
  },
  "author": {
    "name": "KFlash",
    "email": "kflash123@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kflash/sikky/issues"
  },
  "homepage": "https://github.com/kflash/sikky#readme",
  "devDependencies": {
    "rollup": "^0.30.0",
    "rollup-plugin-buble": "^0.10.0",
    "rollup-plugin-uglify": "^0.3.1",
    "rollup-plugin-replace": "^1.1.0",
    "rimraf": "^2.5.2",
    "typescript": "next",
    "tslint": "next",
    "codelyzer": "0.0.22"
  }
}
