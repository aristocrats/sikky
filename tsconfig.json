{
  "compilerOptions": {
    "allowJs": false,
    "allowUnreachableCode": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "newLine": "LF",
    "jsx": "preserve",
    "pretty": true,
    "removeComments": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "suppressImplicitAnyIndexErrors": false,
    "inlineSources": true,
    "inlineSourceMap": true,
    "noEmitHelpers": true
  },
  "exclude": [
    "build",
    "dist",
    "node_modules"
  ],
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": { "rewriteTsconfig": false }
}
